<?xml version="1.0"?>
<Container version="2">
  <Name>YANDA-Etherpad</Name>
  <Repository>etherpad/etherpad</Repository>
  <Registry>https://hub.docker.com/r/yandasoftware/yanda-etherpad</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>true</Privileged>
  <Project>https://etherpad.org/</Project>
  <Overview>This container will download and install Etherpad Lite.</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:9001]</WebUI>
  <TemplateURL>false</TemplateURL>
  <Icon>https://yanda.agency/img/icons/favicon-32x32.png</Icon>
  <ExtraParams>--restart=unless-stopped</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled/>
  <DonateText>If you like my work please</DonateText>
  <DonateLink>https://www.paypal.me/seb_grant</DonateLink>
  <Overview>This container will download and install Jenkins and all the necessary libraries for automatically building and testing PHP and NodeJS based applications.</Overview>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9001</HostPort>
        <ContainerPort>9001</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/jenkins</HostDir>
      <ContainerDir>/jenkins</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>YANDA Collaborative Documents</Value>
      <Name>TITLE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>FAVICON</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>9001</Value>
      <Name>PORT</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value/>
      <Name>ADMIN_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>USER_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>MySQL</Value>
      <Name>DB_TYPE</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>localhost</Value>
      <Name>DB_HOST</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>etherpad</Value>
      <Name>DB_NAME</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>DB_USER</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value></Value>
      <Name>DB_PASS</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="PORT" Target="9001" Default="" Mode="tcp" Description="Container Port: 9001" Type="Port" Display="always" Required="true" Mask="false">9001</Config>
  <Config Name="ADMIN_PASSWORD" Target="" Default="" Mode="tcp" Description="" Display="always" Required="true"/>
  <Config Name="USER_PASSWORD" Target="" Default="" Mode="tcp" Description="" Display="always" Required="true"/>
  <Config Name="DB_USER" Target="" Default="" Mode="tcp" Description="" Display="always" Required="true"/>
  <Config Name="DB_PASS" Target="" Default="" Mode="tcp" Description="" Display="always" Required="true"/>
  
</Container>